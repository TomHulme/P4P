<Canvas x:Class="Chess.Square"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:s="http://schemas.microsoft.com/surface/2008"
    Loaded="Square_Loaded">
    <Grid>
        <Rectangle Name="rectangle" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Panel.ZIndex="5"/>
        <s:TagVisualizer
                    Name="MyTagVisualizer" 
                    VerticalAlignment="Stretch"
                    HorizontalAlignment="Stretch"
                    Background="Crimson"
                    VisualizationAdded="tagVis_VisualizationAdded"
                    VisualizationRemoved="tagVis_VisualizationRemoved"
                    Panel.ZIndex="4"
                    >
        </s:TagVisualizer>
    </Grid>
</Canvas>
